{
  "swagger": "2.0",
  "info": {
    "description": "文档描述",
    "version": "1.0.0",
    "title": "vue-admin-接口文档",
    "termsOfService": "http://www.tmm-tommy.cn:8888/tommy"
  },
  "host": "localhost:8888",
  "basePath": "/admin",
  "tags": [
    {
      "name": "user-controller",
      "description": "User Controller"
    }
  ],
  "paths": {
    "/user/code": {
      "get": {
        "tags": ["user-controller"],
        "summary": "loginCode",
        "operationId": "loginCodeUsingGET",
        "produces": ["*/*"],
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "deprecated": false
      }
    },
    "/user/login": {
      "post": {
        "tags": ["user-controller"],
        "summary": "userLogin",
        "operationId": "userLoginUsingPOST",
        "consumes": ["application/json"],
        "produces": ["*/*"],
        "parameters": [
          {
            "name": "code",
            "description": "登录验证码",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "pwd",
            "description": "用户密码",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "user",
            "description": "用户",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "body",
            "name": "userMap",
            "description": "接收对象",
            "required": true,
            "schema": {
              "type": "object"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object"
            }
          },
          "201": {
            "description": "Created"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "deprecated": false
      }
    }
  }
}
